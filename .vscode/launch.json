{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
  

    

    {
      "name": "Launch Chrome against localhost",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:4000",
      "webRoot": "${workspaceFolder}/nerfstudio/viewer/app"
    },
    {
      "name": "Python: Attach using Process ID",
      "type": "python",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: train",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        "--viewer.no-enable",
        "--logging.event-writer=wandb"
      ]
    },
    {
      "name": "Python: train ngp",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": ["instant_ngp"]
    },
    {
      "name": "Python: train ngp nerfstudio",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        // "--experiment_name=nerfstudio_experiment",
        "--logging.local-writer.max-log-size=0",
        "--viewer.no-enable"
        // viewer
        // "--viewer.enable",
        // "--viewer.zmq-url=tcp://127.0.0.1:6001",
        // "--viewer.websocket-port=8014",
        // "--pipeline.model.randomize-background",
        // "--pipeline.model.cone-angle=0.00390625",
        // "nerfstudio-data"
      ]
    },
    {
      "name": "Python: train ngp-friends",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        "--viewer.enable",
        "--viewer.zmq-url=tcp://127.0.0.1:6001",
        "--viewer.websocket-port=8014",
        "--steps_per_save=1000000",
        "--steps_per_test=1000000",
        "--pipeline.model.density-field-params.base-scale=4",
        "--pipeline.model.density-field-params.num-cascades=1",
        "pipeline.datamanager.train-dataparser:friends-data-parser-config",
        "--pipeline.datamanager.train-dataparser.scene-scale=4"
      ]
    },

    {
      "name": "Python: export SSAN",
      "type": "python",
      "request": "launch",
      "program": "scripts/exporter.py",
      "console": "integratedTerminal",
      "args": [
        "Marching-tet",
        //Change to any preprepared nerfacto model. 
        "--load-config=outputs\\test-sphere\\nerfacto\\2023-04-27_164800/config.yml",
        "--output-dir=exports/tri-tsdf/test-sphere",
        "--use-bounding-box=True",
        "--bounding-box-min", "0.004000000000000031", "-0.25200000000000006", "-0.11099999999999997",
        "--bounding-box-max", "0.30400000000000005", "0.04799999999999993","0.189",
        "--nerf_image_path=ssan/testsphere",

      ]

    },
    
    {
      "name": "Python: export ignatius SSAN",
      "type": "python",
      "request": "launch",
      "program": "scripts/exporter.py",
      "console": "integratedTerminal",
      "args": [
        "Marching-tet",
        //Change to any preprepared nerfacto model. 
        "--load-config=outputs/data/TandT/Ignatius/nerfacto/2023-08-15_135545/config.yml",
        "--output-dir=exports/tri-tsdf/statue",
        "--use-bounding-box=True",
        "--bounding-box-min", "-0.15", "-0.2", "-0.35",
        "--bounding-box-max", "0.15", "0.2","0.35",
        "--nerf_image_path=ssan/teststatue",

      ]

    },
    {
      "name": "Python: export lego SSAN",
      "type": "python",
      "request": "launch",
      "program": "scripts/exporter.py",
      "console": "integratedTerminal",
      "args": [
        "Marching-tet",
        //Change to any preprepared nerfacto model. 
        "--load-config outputs/data/nerf-synthetic/lego/nerfacto/2023-03-24_124319/config.yml",
        "--output-dir=exports/tri-tsdf/lego",
        "--use-bounding-box=True",
        "--bounding-box-min", "-0.3", "-0.4", "-0.75",
        "--bounding-box-max", "0.3", "0.19","-0.25"

      ]

    },

    {
      "name": "Python: train semantic_nerfw",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": ["semantic_nerf"]
    }
  ]
}
